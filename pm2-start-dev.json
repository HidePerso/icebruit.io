{
  "apps": [
    {
      "name": "bruit-io-watch-dev",
      "script": "npm",
      "args": [
        "start"
      ],
      "env": {
        "BRUIT_IO_API_URL": "http://localhost:3334"
      },
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "out_file": "bruit-io-watch-dev.log",
      "error_file": "bruit-io-watch-dev.log"
    }
  ]
}